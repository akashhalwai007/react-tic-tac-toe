{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
    // Add other ESLint configurations or plugins as needed
  ],
  "plugins": ["react"],
  "rules": {
   "react/prop-types": [0, {  }],
   "react/no-unescaped-entities": [0, {}],
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
    // Customize your rules here
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
